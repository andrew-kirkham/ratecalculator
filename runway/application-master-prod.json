{
  "app": {
    "instance_type": "t2.micro",
    "app_description": "Rate Calculator",
    "instance_profile": "akirkham_ratecalculator"
  },
  "elb": {
    "subnet_purpose": "internal",
    "target": "TCP:8080",
    "ports": [
      {"loadbalancer": "HTTP:80", "instance": "HTTP:80"}
    ]
  },
  "asg": {
    "subnet_purpose": "internal",
    "min_inst": 1,
    "max_inst": 3
  },
  "regions": [
    "us-east-1"
  ],
  "deploy_strategy": "redblack"
}
